<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.callback.gymmanagment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gym"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_gym"
        android:supportsRtl="true"
        android:theme="@style/Theme.GymManagment">
        <activity android:name=".AddFormActivity"></activity>
        <activity android:name=".EditActivity" />
        <activity android:name=".WeeklyActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".AllActiviteis" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>